# AWS Configuration
AWS_REGION=ap-northeast-1
AWS_PROFILE=default

# DynamoDB Configuration
DYNAMODB_TABLE_NAME=shopifyshipping-dev-sessions

# Developer Name (used to search for sessions in DynamoDB)
# Not required if SHOPIFY_ACCESS_TOKEN is set
DEVELOPER_NAME=your-name-in-romaji

# Shopify Store URL (without /admin/api/...)
SHOPIFY_STORE_URL=https://shipping-dev-your-name.myshopify.com

# Shopify Access Token (Optional: if set, DynamoDB access is skipped)
# You can find this token in your Shopify app settings or DynamoDB sessions
SHOPIFY_ACCESS_TOKEN=
